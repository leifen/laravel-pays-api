{
    "name": "davidnineroc/laravel-pays-api",
    "description": "PaysApi 集成在 laravel！",
    "keywords": ["DavidNineRoc", "laravel", "PaysApi"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DavidNineRoc",
            "email": "1033404553@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "autoload": {
        "psr-4": {
            "DavidNineRoc\\Payment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "6.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavidNineRoc\\Payment\\PaysApiServiceProvider"
            ],
            "aliases": {
                "PaysApi": "DavidNineRoc\\Payment\\Facades\\PaysApi"
            }
        }
    }
}
